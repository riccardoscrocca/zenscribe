{
  "name": "zenscribe",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clean-macos-files": "find . -name '._*' -type f -delete",
    "predeploy": "npm run clean-macos-files",
    "deploy": "npm run build && netlify deploy --prod --skip-functions-cache"
  },
  "dependencies": {
    "@netlify/functions": "^3.1.2",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/supabase-js": "^2.49.4",
    "@types/busboy": "^1.5.4",
    "axios": "^1.9.0",
    "busboy": "^1.6.0",
    "form-data": "^4.0.2",
    "jsdom": "^26.1.0",
    "jspdf": "^2.5.1",
    "lucide-react": "^0.363.0",
    "node-fetch": "^2.7.0",
    "openai": "^4.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "stripe": "^18.1.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "formdata-node": "^6.0.3",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "vite": "^5.1.6"
  }
}
